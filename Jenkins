pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/Srushti-HL/java1.git'
            }
        }
        stage('Compile') {
            steps {
                sh 'javac src\java1\Srushti.java'
            }
        }
        stage('Run') {
            steps {
                sh 'java src\java1\Srushti'
            }
        }
    }
}
